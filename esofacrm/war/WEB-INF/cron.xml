<?xml version="1.0" encoding="UTF-8"?>
<cronentries>
  <cron>
    <url>/qj/start?setName=dailyPurge&amp;fileName=all</url>
    <description>purge old entries</description>
    <schedule>every day 03:30</schedule>
    <timezone>America/New_York</timezone>
  </cron>  
  <cron>
    <url>/qj/start?setName=snapshotReports</url>
    <description>snapshot report generation</description>
    <schedule>1 of month 01:30</schedule>
    <timezone>America/New_York</timezone>
  </cron>    
  
</cronentries>